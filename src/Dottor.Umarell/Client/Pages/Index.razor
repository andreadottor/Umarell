@page "/"
@attribute [Authorize]

@inject ApiProxyService ApiProxy

<Page Title="Elenco cantieri segnalati">
    
    @if(items != null)
    {
        <MapDisplay Items="items" />
    }

</Page>



@code {
    private IEnumerable<BuildingSiteModel> items;

    protected override async Task OnInitializedAsync()
    {
        items = await ApiProxy.GetAllBuildingSiteAsync();
    }

}


